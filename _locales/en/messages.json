{
  "returnedErrorMessage": {
    "message": "Goo.gl returned error message: $ERRMSG$.",
    "placeholders": {
      "errmsg" : {
        "content" : "$1"
      }
    }
  },
  "copiedToClipboard": {
    "message": "$SHORTURL$ has been copied to the clipboard.  Shortened from $LONGURL$",
    "placeholders": {
      "shorturl" : {
        "content" : "$1"
      },
      "longurl" : {
        "content" : "$2"
      }
    }
  },
  "errorContacting": {
    "message": "Error contacting goo.gl.  Status code: $CODE$",
    "placeholders": {
      "code" : {
        "content" : "$1"
      }
    }
  },
  "creationFailed": {
    "message": "Short URL creation failed: $ERRMSG$",
    "placeholders": {
      "errmsg" : {
        "content" : "$1"
      }
    }
  }
}
