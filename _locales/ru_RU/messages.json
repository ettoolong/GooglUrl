{
  "returnedErrorMessage": {
    "message": "Получено сообщение об ошибке: $ERRMSG$.",
    "placeholders": {
      "errmsg" : {
        "content" : "$1"
      }
    }
  },
  "copiedToClipboard": {
    "message": "Ссылка $LONGURL$ сокращена. Сокращённый адрес $SHORTURL$ скопирован в буфер обмена.",
