{
  "description": "Make short URL by Google's URL shortener.",
  "manifest_version": 2,
  "name": "Goo.gl url",
  "version": "0.0.11",
  "icons": {
    "48": "icon/icon.svg"
  },
  "applications": {
    "gecko": {
      "id": "@googlurl",
      "strict_min_version": "52.0"
    }
  },
  "developer": {
    "name": "Ett Chung",
    "url": "https://github.com/ettoolong/GooglUrl"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "Goo.gl url",
    "default_icon": "icon/icon.svg"
  },
  "default_locale": "en",
  "permissions": [
    "https://www.googleapis.com/urlshortener/*",
    "activeTab",
    "storage",
    "clipboardWrite",
    "contextMenus",
    "notifications"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
